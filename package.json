{
  "name": "graphql-node-server",
  "version": "1.0.0",
  "description": "A graphql node server starter kit.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "pm2 start dist",
    "prestart": "npm run -s build"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Socorro Aguilar <saguilar1991@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "mongoose": "^4.8.5",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "eslint": "^3.16.1",
    "express-graphql": "^0.6.3",
    "graphql": "^0.9.1",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0"
  }
}
